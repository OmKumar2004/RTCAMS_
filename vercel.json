{
    "builds": [{
        "src": "attendance_System/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime" : "python3.7" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "attendance_System/wsgi.py"
        }
    ]
}